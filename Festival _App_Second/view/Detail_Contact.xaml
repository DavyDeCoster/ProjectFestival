            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="1*"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*"/>
            <ColumnDefinition Width="1*"/>
        </Grid.ColumnDefinitions>
        <Border BorderBrush="Black" BorderThickness="1" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="79.094" Height="79.094" Grid.RowSpan="2"/>
        <TextBlock HorizontalAlignment="Left" Height="31.026" Margin="126.154,10,0,0"  VerticalAlignment="Top" Width="133.333" Grid.ColumnSpan="2"><Run Language="nl-be" Text="Naam:"/></TextBlock>
        <TextBox HorizontalAlignment="Left" Height="31.026" Margin="10.001,10,0,0"  TextWrapping="Wrap" VerticalAlignment="Top" Width="147.18" Grid.Column="1"/>
